<body>
	<!-- Button trigger modal-->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalCart">Launch modal</button>

<!-- Modal: modalCart -->
<div class="modal fade" id="modalCart" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <!--Header-->
      <div class="modal-header">
        <h4 class="modal-title " id="myModalLabel" >Action Allocation</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">×</span>
        </button>
      </div>
      <!--Body-->
      <div class="modal-body">
			<select class="mdb-select md-form colorful-select dropdown-danger">
  <option value="" disabled selected>Choose your option</option>
  <option value="1">Option 1</option>
  <option value="2">Option 2</option>
  <option value="3">Option 3</option>
</select>
	</div>
      <!--Footer-->
      <div class="modal-footer">
        <button class="btn btn-primary">Assign</button>
      </div>
    </div>
  </div>
</div>
<!-- Modal: modalCart -->

</body>
<script type="text/javascript" src="js/jquery-3.4.1.min.js"></script>
